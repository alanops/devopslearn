{
  "name": "devopslearn",
  "version": "0.1.0",
  "description": "DevOps Learning Platform - Learn by Fixing Broken Things",
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run build:server && npm run setup:public",
    "build:server": "cd src/server && npm install && npm run build",
    "setup:public": "mkdir -p src/server/public && cp -r out/* src/server/public/",
    "start": "next start",
    "start:server": "cd src/server && npm start",
    "export": "next export",
    "lint": "next lint",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "railway:start": "npm run build && cd src/server && node dist/index.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.3.0",
    "axios": "^1.6.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "formidable": "^3.5.4",
    "lambda-multipart-parser": "^1.0.1",
    "lucide-react": "^0.309.0",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "sharp": "^0.33.2",
    "socket.io-client": "^4.7.2",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/formidable": "^3.4.5",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "postcss": "8.4.33",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  }
}
